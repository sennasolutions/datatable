{
    "name": "senna/datatable",
    "description": "Datatable for Senna",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lennard",
            "email": "packagist@spring.nl"
        }
    ],
    "require": {
        "php": "^8.0",
        "senna/ui": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Senna\\Datatable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Senna\\Datatable\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^7.7|^8.0",
        "phpunit/phpunit": "^9.4"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Senna\\Datatable\\DatatableServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox",
        "test-f": "vendor/bin/phpunit --filter"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../utils"
        },
        {
            "type": "path",
            "url": "../ui"
        },
        {
            "type": "path",
            "url": "../cms"
        },
        {
            "type": "path",
            "url": "../package-tools"
        },
        {
            "type": "path",
            "url": "../media"
        },
        {
            "type": "path",
            "url": "../admin"
        }
    ]
}
