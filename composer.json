{
    "name": "senna/datatable",
    "description": "Datatable for Senna",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lennard",
            "email": "packagist@spring.nl"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Senna\\Datatable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Senna\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.4"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Senna\\Datatable\\DatatableServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox",
        "test-f": "vendor/bin/phpunit --filter"
    }
}
